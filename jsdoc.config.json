{
	"plugins": ["plugins/markdown", "plugins/summarize"],
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc"]
	},
	"opts": {
		"destination": "./docs/",
		"readme": "./README.md",
		"encoding": "utf8",
		"recurse": true,
		"verbose": true,
		"template": "./node_modules/docdash"
	},
	"docdash": {
		"search": true,
		"sort": true,
		"collapse": true
	},
	"templates": {
		"default": {
			"includeDate": false,
			"outputSourceFiles": false
		}
	}
}
